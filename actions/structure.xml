<?xml version="1.0" encoding="utf-8"?>
<extension name="Process" url="/wfEngine/Process/index" level="6">
	<sections>
		<section name="Manage Process" url="/wfEngine/Process/index">
			<trees>
				<tree name="Process library" 
					dataUrl="/wfEngine/Process/getOntologyData" 
					editClassUrl="/wfEngine/Process/editProcessClass"
					editInstanceUrl="/wfEngine/Process/editProcess"
					addInstanceUrl="/wfEngine/Process/addInstance"
					addSubClassUrl="/wfEngine/Process/addProcessClass"
					deleteUrl="/wfEngine/Process/delete"
					duplicateUrl="/wfEngine/Process/cloneInstance"
				/>
			</trees>
			<actions>
				<action name="search" url="/wfEngine/Process/search" context="*" />
				<action name="new class" js="GenerisAction.subClass" url="/wfEngine/Process/addProcessClass" context="class" />
				<action name="new process" js="GenerisAction.instanciate" url="/wfEngine/Process/addInstance" context="class" />
				<action name="delete" js="GenerisAction.removeNode" url="/wfEngine/Process/delete" context="resource" />
				<action name="duplicate" js="GenerisAction.duplicateNode" url="/wfEngine/Process/cloneInstance" context="instance" />
				<action name="edit process" url="/wfEngine/Process/editProcess" context="instance" />
				<action name="new property" js="GenerisAction.addProperty" url="/wfEngine/Process/addClassProperty" context="class" />
				<action name="meta data" js="GenerisAction.loadMetaData" url="/taoTests/Tests/getMetaData"  context="instance" />
				<action name="create table" js="GenerisAction.resultTable" url="/taoResults/Results/createTable"  context="class" />
				<!--<action name="import" url="/wfEngine/Process/import"  context="class" />-->
				<action name="export" url="/wfEngine/Process/export"  context="class" reload="true" />
			</actions>
		</section>
	</sections>
</extension>